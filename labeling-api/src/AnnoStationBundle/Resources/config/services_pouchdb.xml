<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="annostation.labeling_api.monitoring.check.security_document_existence_in_task_databases"
                 class="AnnoStationBundle\Service\Monitoring\Check\SecurityDocumentExistenceInTaskDatabases">
            <argument type="service" id="annostation.labeling_api.database.facade.labeling_task"/>
            <argument type="service" id="annostation.labeling_api.service.task_database_creator"/>
            <argument type="service" id="guzzle.client"/>
            <argument>%couchdb_user%</argument>
            <argument>%couchdb_password%</argument>
            <argument>%couchdb_host%</argument>
            <argument>%couchdb_port%</argument>
            <tag name="liip_monitor.check" alias="security_document_existence_in_task_databases" group="default"/>
        </service>

        <service id="annostation.labeling_api.database.facade.factory.task_database.labeled_thing"
                 class="AnnoStationBundle\Database\Facade\Factory\TaskDatabase\LabeledThing">
            <argument type="service" id="annostation.labeling_api.database.facade.labeled_thing"/>
            <argument type="service" id="annostation.services.database_document_manager_factory"/>
            <argument type="service" id="annostation.labeling_api.service.task_database_creator"/>
            <argument>%database_name_read_only%</argument>
        </service>
        <service id="annostation.labeling_api.database.facade.factory.task_database.labeled_thing_in_frame"
                 class="AnnoStationBundle\Database\Facade\Factory\TaskDatabase\LabeledThingInFrame">
            <argument type="service" id="annostation.labeling_api.database.facade.labeled_thing_in_frame"/>
            <argument type="service" id="annostation.services.database_document_manager_factory"/>
            <argument type="service" id="annostation.labeling_api.service.task_database_creator"/>
            <argument>%database_name_read_only%</argument>
        </service>
        <service id="annostation.labeling_api.database.facade.factory.task_database.labeled_thing_group"
                 class="AnnoStationBundle\Database\Facade\Factory\TaskDatabase\LabeledThingGroup">
            <argument type="service" id="annostation.labeling_api.database.facade.labeled_thing_group"/>
            <argument type="service" id="annostation.services.database_document_manager_factory"/>
            <argument type="service" id="annostation.labeling_api.service.task_database_creator"/>
            <argument>%database_name_read_only%</argument>
        </service>
        <service id="annostation.labeling_api.database.facade.factory.task_database.labeling_task"
                 class="AnnoStationBundle\Database\Facade\Factory\TaskDatabase\LabelingTask">
            <argument type="service" id="annostation.labeling_api.database.facade.labeling_task"/>
            <argument type="service" id="annostation.services.database_document_manager_factory"/>
            <argument type="service" id="annostation.labeling_api.service.task_database_creator"/>
            <argument>%database_name_read_only%</argument>
        </service>
        <service id="annostation.labeling_api.database.facade.factory.task_database.project"
                 class="AnnoStationBundle\Database\Facade\Factory\TaskDatabase\Project">
            <argument type="service" id="annostation.labeling_api.database.facade.project"/>
            <argument type="service" id="annostation.services.database_document_manager_factory"/>
            <argument type="service" id="annostation.labeling_api.service.task_database_creator"/>
            <argument>%database_name_read_only%</argument>
        </service>
        <service id="annostation.labeling_api.database.facade.factory.task_database.labeled_frame"
                 class="AnnoStationBundle\Database\Facade\Factory\TaskDatabase\LabeledFrame">
            <argument type="service" id="annostation.labeling_api.database.facade.labeled_frame"/>
            <argument type="service" id="annostation.services.database_document_manager_factory"/>
            <argument type="service" id="annostation.labeling_api.service.task_database_creator"/>
            <argument>%database_name_read_only%</argument>
        </service>

        <service id="annostation.labeling_api.database.facade.factory.labeled_thing"
                 alias="annostation.labeling_api.database.facade.factory.task_database.labeled_thing"/>
        <service id="annostation.labeling_api.database.facade.factory.labeled_thing_in_frame"
                 alias="annostation.labeling_api.database.facade.factory.task_database.labeled_thing_in_frame"/>
        <service id="annostation.labeling_api.database.facade.factory.labeled_thing_group"
                 alias="annostation.labeling_api.database.facade.factory.task_database.labeled_thing_group"/>
        <service id="annostation.labeling_api.database.facade.factory.labeling_task"
                 alias="annostation.labeling_api.database.facade.factory.task_database.labeling_task"/>
        <service id="annostation.labeling_api.database.facade.factory.project"
                 alias="annostation.labeling_api.database.facade.factory.task_database.project"/>
        <service id="annostation.labeling_api.database.facade.factory.labeled_frame"
                 alias="annostation.labeling_api.database.facade.factory.task_database.labeled_frame"/>

    </services>
</container>
