<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="annostation.labeling_api.command.init" class="AppBundle\Command\Init">
            <argument type="service" id="doctrine_couchdb.client.default_connection"/>
            <argument type="service" id="annostation.labeling_api.service.video_importer"/>
            <argument type="service" id="annostation.labeling_api.service.task_creator"/>
            <argument>%database_name%</argument>
            <argument>%user_password%</argument>
            <argument>%cache_dir%</argument>
            <argument>%frame_cdn_dir%</argument>
            <argument type="service" id="annostation.labeling_api.database.facade.user"/>
            <argument type="service" id="annostation.labeling_api.database.facade.project"/>
            <argument type="service" id="annostation.labeling_api.database.facade.labeling_group"/>
            <argument type="service" id="annostation.labeling_api.database.facade.task_configuration"/>
            <argument type="service" id="annostation.labeling_api.service.task_configuration_xml_converter_factory"/>
            <tag name="console.command"/>
        </service>

        <service id="annostation.labeling_api.command.import_video_command" class="AppBundle\Command\ImportVideo">
            <argument type="service" id="annostation.labeling_api.database.facade.project"/>
            <argument type="service" id="annostation.labeling_api.service.video_importer"/>
            <argument type="service" id="annostation.labeling_api.service.task_creator"/>
            <tag name="console.command"/>
        </service>
        <service id="annostation.labeling_api.command.import_videos_command" class="AppBundle\Command\ImportVideos">
            <argument type="service" id="annostation.labeling_api.database.facade.project"/>
            <argument type="service" id="annostation.labeling_api.service.video_importer"/>
            <argument type="service" id="annostation.labeling_api.service.task_creator"/>
            <argument type="service" id="annostation.labeling_api.database.facade.user"/>
            <tag name="console.command"/>
        </service>

        <service id="annostation.labeling_api.command.worker_starter" class="AppBundle\Command\WorkerStarter">
            <argument type="service" id="annostation.labeling_api.service.amqp_pool_config"/>
            <argument type="service" id="annostation.labeling_api.vendor.logger"/>
            <tag name="console.command"/>
        </service>
        <service id="annostation.labeling_api.command.rabbit_mq" class="AppBundle\Command\RabbitMq">
            <argument type="service" id="annostation.labeling_api.service.amqp_pool_config"/>
            <tag name="console.command"/>
        </service>
        <service id="annostation.labeling_api.command.queue_maintainer" class="AppBundle\Command\QueueMaintainer">
            <argument type="service" id="annostation.labeling_api.service.amqp_pool_config"/>
            <argument type="service" id="annostation.labeling_api.vendor.worker_pool.amqp"/>
            <tag name="console.command"/>
        </service>
        <service id="annostation.labeling_api.command.project_to_project_task_mover" class="AppBundle\Command\ProjectToProjectTaskMover">
            <argument type="service" id="annostation.labeling_api.database.facade.labeling_task" />
            <argument type="service" id="annostation.labeling_api.database.facade.project" />
            <tag name="console.command"/>
        </service>
        <service id="annostation.labeling_api.command.create_label_structure"
                 class="AppBundle\Command\CreateLabelStructure">
            <tag name="console.command"/>
        </service>
        <service id="annostation.labeling_api.command.video_calibration_to_calibration_doc"
                 class="AppBundle\Command\VideoCalibrationToCalibrationDoc">
            <argument type="service" id="annostation.labeling_api.database.facade.video" />
            <argument type="service" id="annostation.labeling_api.database.facade.calibration_data" />
            <tag name="console.command"/>
        </service>
        <service id="annostation.labeling_api.command.import_labeling_csv_file"
                 class="AppBundle\Command\ImportLabelingCsvFile">
            <argument type="service" id="annostation.labeling_api.database.facade.project"/>
            <argument type="service" id="annostation.labeling_api.database.facade.video"/>
            <argument type="service" id="annostation.labeling_api.database.facade.labeling_task"/>
            <argument type="service" id="annostation.labeling_api.service.label_structure"/>
            <argument type="service" id="annostation.labeling_api.service.label_importer.csv"/>
            <tag name="console.command"/>
        </service>
        <service id="annostation.labeling_api.command.labeling_task_remove_predefined_classes"
                 class="AppBundle\Command\LabelingTaskRemovePredefinedClasses">
            <argument type="service" id="annostation.labeling_api.database.facade.labeling_task"/>
            <tag name="console.command"/>
        </service>
        <service id="annostation.labeling_api.command.add_default_project_user_id"
                 class="AppBundle\Command\AddDefaultProjectUserId">
            <argument type="service" id="annostation.labeling_api.database.facade.project"/>
            <argument type="service" id="annostation.labeling_api.database.facade.user"/>
            <tag name="console.command"/>
        </service>
        <service id="annostation.labeling_api.command.assign_default_labeling_group"
                 class="AppBundle\Command\AssignDefaultLabelingGroup">
            <argument type="service" id="annostation.labeling_api.database.facade.project"/>
            <argument type="service" id="annostation.labeling_api.database.facade.labeling_group"/>
            <tag name="console.command"/>
        </service>
        <service id="annostation.labeling_api.command.project.cleanup_batch_upload_directory"
                 class="AppBundle\Command\Project\CleanupBatchUploadDirectory">
            <argument>%annostation.labeling_api.project_batch_upload_cache_dir%</argument>
            <tag name="console.command"/>
        </service>

        <service id="annostation.labeling_api.command.set_missing_label_instructions_in_projects" class="AppBundle\Command\SetMissingLabelInstructionsInProjects">
            <argument type="service" id="annostation.labeling_api.database.facade.project"/>
            <argument type="service" id="annostation.labeling_api.database.facade.labeling_task"/>
            <tag name="console.command"/>
        </service>
    </services>
</container>
