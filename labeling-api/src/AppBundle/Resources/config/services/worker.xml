<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="annostation.labeling_api.worker.job_instruction.video" class="AppBundle\Worker\JobInstruction\VideoFrameSplitter">
            <argument type="service" id="annostation.labeling_api.service.video.video_frame_splitter" />
            <argument type="service" id="annostation.labeling_api.database.facade.video" />
            <argument type="service" id="annostation.labeling_api.database.facade.labeling_task" />
            <argument type="service" id="oneup_flysystem.cache_filesystem" />
            <argument>%cache_dir%</argument>
        </service>

        <service id="annostation.labeling_api.worker.job_instruction.kitti_exporter" class="AppBundle\Worker\JobInstruction\KittiExporter">
            <argument type="service" id="annostation.labeling_api.service.task_exporter.kitti" />
            <argument type="service" id="annostation.labeling_api.database.facade.labeling_task" />
        </service>

        <service id="annostation.labeling_api.worker.job_instruction.interpolation" class="AppBundle\Worker\JobInstruction\Interpolation">
            <argument type="service" id="annostation.labeling_api.service.interpolation" />
            <argument type="service" id="annostation.labeling_api.database.facade.labeled_thing" />
            <argument type="service" id="annostation.labeling_api.database.facade.status" />
        </service>
    </services>
</container>
