          "fieldSetReferences": {
            "general": {
              "id": "file-existence"
            }
          }
        }
      },
      {
        "type": "filename-regex-tagger",
        "config": {
          "\\.lane\\.xml$": {
            "sensingModule": "Lane"
          },
          "\\.lsr\\.xml$": {
            "sensingModule": "LSR"
          },
          "\\.pscw\\.xml$": {
            "sensingModule": "PSCW"
          },
          "\\.tsa\\.xml$": {
            "sensingModule": "TSA"
          },
          "\\.tsr\\.xml$": {
            "sensingModule": "TSR"
          },
          "\\.od\\.xml$": {
            "sensingModule": "OD"
          },
          "\\.pd\\.xml$": {
            "sensingModule": "PD"
          },
          "\\.vd\\.xml$": {
            "sensingModule": "VD"
          }
        }
      },
      {
        "type": "project",
        "config": {}
      },
      {
        "type": "anno_station",
        "config": {
          "fileExtension": ".anno.xml",
          "fieldSetReferences": {
            "metaData": {
              "id": "anno-station-metadata"
            }
          }
        }
      },
      {
        "type": "can",
        "config": {
          "fieldSetReferences": {
            "general": {
              "id": "can",
              "position": 0
            }
          },
          "fileExtension": ".mdex",
          "schema": "@DataStationBundle/Resources/assets/mdex.schema.xml",
          "attributeActions": {
            "Maneuver": {
              "Driven Distance": {
                "customRanges": {
                  "value": {
                    "boundaries": [
                      "-infty",
                      0,
                      100,
                      1000,
                      10000,
                      100000,
                      1000000
                    ]
                  }
                }
              },
              "Scene Speed Mean": {
                "customRanges": {
                  "value": {
                    "boundaries": [
                      "-infty",
                      0,
                      0,
                      15,
                      25,
                      35,
                      45,
                      55,
                      65,
                      75,
                      85,
                      105,
                      125,
                      145,
                      165,
                      185,
                      205,
                      "infty"
                    ],
                    "types": [
                      "exclusive",
                      "exclusive",
                      "inclusive"
                    ]
                  }
                }
              }
            },
            "Recording Information": {
              "Video Length (Frames)": {
                "customRanges": {
                  "value": {
                    "boundaries": [
                      0,
                      100,
                      200,
                      300,
                      400,
                      500,
                      600,
                      700,
                      800,
                      900,
                      1000,
                      1200,
                      1400,
                      1600,
                      1800,
                      2000,
                      2200,
                      2400,
                      2600,
                      2800,
                      3000,
                      3500,
                      4000,
                      4500,
                      5000,
                      5500,
                      6000,
                      6500,
                      7000,
                      7500,
                      8000,
                      8500,
                      9000,
                      9500,
                      10000
                    ]
                  }
                }
              },
              "Video Length (Seconds)": {
                "customRanges": {
                  "value": {
                    "boundaries": [
                      0,
                      0,
                      1,
                      10,
                      20,
                      30,
                      40,
                      50,
                      100,
                      1000,
                      "infty"
                    ]
                  }
                }
              }
            }
          }
        }
      },
      {
        "type": "recording",
