  "settings": {
    "annostation.requirement_xmls": "@DataStationBundle/Resources/assets/autoliv_requirements.xml"
  },
  "recordingSpecification": {},
  "checkInSpecification": {
    "groupingStrategy": "autoliv",
    "groupingStrategyConfig": {
      "pattern": "/^(?<recordingId>(?<camera>[^_]+)[_-]{1}(?<licensePlate>\\w+(?:-\\w+)?(?:-\\w+)?)_(?<recordingTime>[0-9]{8}_[0-9]{6})).*$/",
      "type": "RegexBasedGroupingStrategy"
    },
    "fileConfig": {
      "defaultClassification": {
        "key": "active",
        "label": "default"
      },
      "classifications": {
        "ignored": {
          "patterns": [
            ".*_CanCheck\\.csv$"
          ],
          "label": "Ignored"
        }
      },
      "fileTypes": {
        "pdLabelFile": {
          "patterns": [
            "\\.pd\\.xml$"
          ],
          "label": "pd"
        },
        "vdLabelFile": {
          "patterns": [
            "\\.vd\\.xml$"
          ],
          "label": "vd"
        },
        "tsrLabelFile": {
          "patterns": [
            "\\.tsr\\.xml$"
          ],
          "label": "tsr"
        },
        "tsaLabelFile": {
          "patterns": [
            "\\.tsa\\.xml$"
          ],
          "label": "tsa"
        },
        "lsrLabelFile": {
          "patterns": [
            "\\.lsr\\.xml$"
          ],
          "label": "lsr"
        },
        "pscwLabelFile": {
          "patterns": [
            "\\.pscw\\.xml$"
          ],
          "label": "pscw"
        },
        "laneLabelFile": {
          "patterns": [
            "\\.lane\\.xml$"
          ],
          "label": "lane"
        },
        "odLabelFile": {
          "patterns": [
            "\\.od\\.xml$"
          ],
          "label": "od"
        },
        "jsonFile": {
          "patterns": [
            "\\.json$"
          ],
          "label": "json"
        },
        "canFile": {
          "patterns": [
            "\\.mdex$"
          ],
          "label": "mdex"
        },
        "datFile": {
          "patterns": [
            "\\.dat$"
          ],
          "label": "dat"
        },
        "annoFile": {
          "patterns": [
            "\\.anno\\.xml$"
          ],
          "label": "anno"
        }
      }
    }
  },
  "dataExtractorSpecification": {
    "dataExtractors": [
      {
        "type": "filetype-existence",
        "config": {
          "fileTypes": {
            "datFile": {
              "label": "Recording with dat File"
            },
            "annoFile": {
              "label": "Recording with AnnoStation File"
            },
            "jsonFile": {
              "label": "Recording with JSON File"
            },
            "canFile": {
              "label": "Recording with CAN File"
            },
            "laneLabelFile": {
              "label": "Recording with Lane Labelfile"
            },
            "tsrLabelFile": {
              "label": "Recording with TSR Labelfile"
            },
            "tsaLabelFile": {
              "label": "Recording with TSA Labelfile"
            },
            "lsrLabelFile": {
              "label": "Recording with LSR Labelfile"
            },
            "odLabelFile": {
