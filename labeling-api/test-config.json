{
  "roles": [
    {
      "name": "Admin",
      "permissions": ["SEARCH", "SEARCH.SAVE", "SEARCH.DOWNLOAD", "SEARCH.DELETE", "GLOBAL_ADMIN_AREA", "ADMIN"],
      "system": true
    },
    {
      "name": "User",
      "permissions": ["SEARCH", "SEARCH.SAVE", "SEARCH.DOWNLOAD", "SEARCH.DELETE"]
    }
  ],
  "users": [
    {
      "username": "user",
      "email": "user@example.org",
      "roles": ["User"]
    },
    {
      "username": "admin",
      "email": "admin.example.org",
      "roles": ["Admin"]
    }
  ],
  "recordingSpecification": {},
  "checkInSpecification": {
    "groupingStrategy": "test",
    "groupingStrategyConfig": {
      "pattern": "/^(?<prefix>test)_(?<recordingId>.+)\\..*$/",
      "type": "RegexBasedGroupingStrategy"
    },
    "fileConfig": {}
  },
  "dataExtractorSpecification": {
    "dataExtractors": [
      {
        "type": "filename-regex-tagger",
        "config": {
          ".*": {
            "test": true
          }
        }
      },
      {
        "type": "project",
        "config": {}
      },
      {
        "type": "recording",
        "config": {
          "fieldSetReferences": {
            "general": {
              "id": "general",
              "position": 0
            }
          }
        }
      },
      {
        "type": "recordingPart",
        "config": {
          "fieldSetReferences": {
            "general": {
              "id": "general",
              "position": 1
            }
          }
        }
      },
      {
        "type": "checkIn",
        "config": {
          "fieldSetReferences": {
            "general": {
              "id": "general"
            }
          }
        }
      },
      {
        "type": "tags",
        "config": {}
      },
      {
        "type": "listing-metadata",
        "config": {
          "mapping": {
            "prefix": {
              "type": "string"
            }
          }
        }
      }
    ]
  },
  "csvExportSpecification": {
    "columns": [
      {
        "path": "filePath",
        "label": "Filepath"
      },
      {
        "path": "data.recording.identifier",
        "label": "Recording Identifier"
      },
      {
        "path": "data.listing-metadata.camera",
        "label": "Camera"
      },
      {
        "path": "data.listing-metadata.licensePlate",
        "label": "License Plate"
      },
      {
        "path": "data.checkIn.checkInTime",
        "label": "CheckIn Time"
      },
      {
        "path": "frameRange.start",
        "label": "Framerange (Start)"
      },
      {
        "path": "frameRange.end",
        "label": "Framerange (End)"
      }
    ]
  },
  "playlistExportSpecification": {
    "rangeDelimiter": "|"
  },
  "fieldSets": [
    {
      "id": "general",
      "name": "Allgemein"
    },
    {
      "id": "file-existence",
      "name": "Datei-Existenz"
    }
  ]
}
