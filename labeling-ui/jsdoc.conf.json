{
  "source": {
    "include": ["Application/"],
    "exclude": ["Application/Vendor"],
    "includePattern": ".+\\.js$"
  },
  "tags": {
    "allowUnknownTags" : true
  },
  "opts": {
    "destination": "Documentation/JavaScript",
    "template": "node_modules/jaguarjs-jsdoc",
    "encoding": "utf8",
    "recurse": true
  },
  "plugins": ["plugins/markdown"],
  "markdown": {
    "parser": "gfm",
    "hardwrap": true,
    "tags": ["examples"]
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "default": {
      "outputSourceFiles" : true
    },
    "applicationName": "AnnoStation",
    "disqus": "",
    "googleAnalytics": "",
    "openGraph": {
      "title": "",
      "type": "website",
      "image": "",
      "site_name": "",
      "url": ""
    },
    "meta": {
      "title": "HAGLA AnnoStation",
      "description": "Annotation Station Software for HAGLA.",
      "keyword": ""
    },
    "linenums": true
  }
}
